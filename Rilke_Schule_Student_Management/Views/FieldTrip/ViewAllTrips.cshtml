@model IEnumerable<Rilke_Schule_Student_Management.Models.SignUp>

@{
    ViewBag.Title = "ViewAllTrips";
}

<h2>ViewAllTrips</h2>

<p>
    @Html.ActionLink("Back", "SelectFieldTrip")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FieldTrip.Teacher_Id)
        </th>
        <th>
            Student Name
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Chaperone_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Chaperone_Contact_Number)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Emergency_Contact_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Emergency_Contact_Number)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Date_Signed)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Printed_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Signed_Bit)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FieldTrip.Teacher_Id)
        </td>
        <td>
            @Html.DisplayFor(model => item.Class.Student.Stud_F_Name) @Html.DisplayFor(model => item.Class.Student.Stud_L_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Chaperone_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Chaperone_Contact_Number)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Emergency_Contact_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Emergency_Contact_Number)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Date_Signed)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Printed_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Signed_Bit)
        </td>
    </tr>
}

@for(int i = 0; i < ViewBag.studentName; i++)
{
    @ViewBag.studentName.Find(i).ToString();
}

</table>
